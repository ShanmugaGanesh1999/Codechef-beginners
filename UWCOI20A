#include <stdio.h>

int main(void) {
    
	int test;
	scanf("%d", &test);
	
	while(test--) {
	    
	    int n, height, max_height = 0;
	    scanf("%d", &n);
	    
	    while(n--) {
	        
	        scanf("%d", &height);
	        
	        if(height > max_height)
	            max_height = height;
	    }
	    
	    printf("%d\n", max_height);
	}
	
	return 0;
}

