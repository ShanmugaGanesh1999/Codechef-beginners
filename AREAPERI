#include<stdio.h>
#include<string.h>

int main()
{
	int l;
	int b;
	scanf("%d", &l);
	scanf("%d", &b);

	int area = l*b;
	int peri = 2*(l+b);

	if(area > peri)
        printf("Area\n%d", area);
    else if(area < peri)
        printf("Peri\n%d", peri);
    else
        printf("Eq\n%d", area);

    return 0;
}
