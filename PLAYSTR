#include<stdio.h>
int main()
{
    int T,i;
    scanf("%d",&T);
    while(T--)
    {
        int N,r=0,s=0;
        scanf("%d",&N);
        char R[N],S[N];
        scanf("%s",S);
        scanf("%s",R);
        for(i=0;i<N;i++)
        {
           if(R[i]=='1')
                r++;
           if(S[i]=='1')
                s++;
        }
        if(r==s)
            printf("YES\n");
        else printf("NO\n");
    }
}
