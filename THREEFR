#include <stdio.h>

int main(void) {
	// your code goes here
	int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        int x,y,z;
        scanf("%d%d%d",&x,&y,&z);
        if(x+y==z||y+z==x||x+z==y){
            printf("yes\n");
        }
        else{
            printf("no\n");
        }
    }
	return 0;
}

