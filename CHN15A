#include<stdio.h>
int main()
{
    int t,n,k,a[100],i,j,count ; 

    scanf("%d",&t) ;
    for(i=0;i<t;i++)
    {
        count = 0 ;
        scanf("%d",&n) ;
        scanf("%d",&k) ;

        for(j=0;j<n;j++)
        {
            scanf("%d",&a[j] ) ;
            a[j] = a[j] + k ;
            if(a[j]%7==0)
            count++ ;
        }


        printf("%d\n",count) ;
    }

    return 0 ;
}
