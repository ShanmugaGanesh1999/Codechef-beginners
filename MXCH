#include <stdio.h>

int main(void) {
    long int t;
    scanf("%ld",&t);
    while(t--){
        int n,k;
        scanf("%d%d",&n,&k);
        int i,a[n];
        for(i=0;i<n;i++)
            a[i]=i+1;
        int temp=a[k];
        a[k]=a[n-1];
        a[n-1]=temp;
    for(i=0;i<n;i++)
        printf("%d ",a[i]);
        printf("\n");
    }
	return 0;
}

