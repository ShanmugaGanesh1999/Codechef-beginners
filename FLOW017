#include <stdio.h>

int main(void) {
	int t;
	long long int a,b,c;
	scanf("%d",&t);
	while(t--){
	    scanf("%lld %lld %lld",&a,&b,&c);
	    if(a>=b && a>=c){
	        if(b>c)
	            printf("%lld",b);
	        else
	            printf("%lld",c);
	    }   
	   else if(b>=a && b>=c){
	       if(a>c)
	            printf("%lld",a);
	       else
	            printf("%lld",c);
	   }
	        
	   else if (c>=a && c>=b){
	       if(a>b)
	        printf("%lld",a);
	        else
	        printf("%lld",b);
	   }
	       
	 printf("\n");
	}
	
	return 0;
}

