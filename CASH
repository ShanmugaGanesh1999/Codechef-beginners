#include<stdio.h>
int main()
{
    long long t,i,n,m,sum=0;
    scanf("%lld",&t);
    while(t--)
    {
        sum=0;
       scanf("%lld%lld",&n,&m);
       long long  a[n];
       for(i=0;i<n;i++)
       {
           scanf("%lld",&a[i]);
           sum+=a[i];
       }
       printf("%lld\n",sum%m);
    }
}
