#include <stdio.h>

int main(void) {
	
	int t;
	scanf("%d",&t);
	int a[100];
	for(int i=0;i<t;i++){
	    int n;
	    scanf("%d",&n);
	    for(int i=0;i<n;i++){
	        scanf("%d",&a[i]);
	    }
	    int min=a[0];
	    for(int i=0;i<n;i++){
	    if(min>a[i]){
	        min=a[i];
	    }
	    
	}
	printf("%d\n",min);
	}
	return 0;
}

