#include<stdio.h>
int main()
{
int T,N,n,lc=0;
 
scanf("%d",&T);
 
for(lc;lc<T;lc++)
    {
    scanf("%d",&N);
    n= N%8;
    if(n==1)
    printf("%dLB\n",N+3);
    else if (n==2)
    printf("%dMB\n",N+3);
    else if (n==3)
    printf("%dUB\n",N+3);
    else if (n==4)
    printf("%dLB\n",N-3);
    else if (n==5)
    printf("%dMB\n",N-3);
    else if (n==6)
    printf("%dUB\n",N-3);
    else if (n==7)
    printf("%dSU\n",N+1);
    else
    printf("%dSL\n",N-1);
    }
 return 0;
}
