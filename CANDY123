#include "stdio.h"
#include "math.h"

int main(int argc, char const *argv[])
{
    int T;
    scanf("%d", &T);
    int A, B;
    while(T--){
        scanf("%d %d", &A, &B);
        int a = sqrt(A);
        int b = (sqrt(4*B+1)-1)/2;
        if (b >= a){
            printf("Bob\n");
        }else{
            printf("Limak\n");
        }
    }
    return 0;
}
