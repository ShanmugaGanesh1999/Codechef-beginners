#include <stdio.h>

int main(void) {
    int i,c,n;
    scanf("%d",&i);
    while(i--){
        c=0;scanf("%d",&n);
        while(n>0){
        if(n>=2048){
            c++;n-=2048;
        }else if(n>=1024){
            c++;n-=1024;
        }else if(n>=512){
            c++;n-=512;
        }else if(n>=256){
            c++;n-=256;
        }else if(n>=128){
            c++;n-=128;
        }else if(n>=64){
            c++;n-=64;
        }else if(n>=32){
            c++;n-=32;
        }else if(n>=16){
            c++;n-=16;
        }else if(n>=8){
            c++;n-=8;
        }else if(n>=4){
            c++;n-=4;
        }else if(n>=2){
            c++;n-=2;
        }else if(n>=1){
            c++;n-=1;
        }
    }printf("%d\n",c);}
	return 0;
}

