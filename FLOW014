#include<stdio.h>
int main(void)
{
    int t;
    float h,c,ten;
    int count;
    scanf("%d",&t);
    while(t--)
    {
        count=0;
        scanf("%f %f %f",&h,&c,&ten);
        if(h>50&&c<0.7&&ten>5600)
            printf("10\n");
        else if(h>50&&c<0.7)
            printf("9\n");
        else if(c<0.7&&ten>5600)
            printf("8\n");
        else if(h>50&&ten>5600)
            printf("7\n");
        else if(h>50||c<0.7||ten>5600)
            printf("6\n");
        else
            printf("5\n");
    }
    return 0;
}
